{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "nyashyker_translate",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "comment"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_header"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_empty"
                  }
                },
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "SYMBOL",
                    "name": "comment"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_empty"
          }
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "part"
          }
        }
      ]
    },
    "_header": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_empty"
            }
          },
          {
            "type": "SYMBOL",
            "name": "works"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "REPEAT",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_empty"
                    }
                  },
                  {
                    "type": "SYMBOL",
                    "name": "credits"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "comment": {
      "type": "PATTERN",
      "value": "(=\\n)|(=[^\\n=].*\\n)"
    },
    "_empty": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "works": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "name"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "comment"
              }
            },
            {
              "type": "SYMBOL",
              "name": "link"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "comment"
              }
            }
          ]
        }
      }
    },
    "link": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "work_marker"
        },
        {
          "type": "PATTERN",
          "value": "https:\\/\\/[ -~]+\\n"
        }
      ]
    },
    "name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "work_marker"
        },
        {
          "type": "PATTERN",
          "value": "[ -~]+\\n"
        }
      ]
    },
    "work_marker": {
      "type": "STRING",
      "value": "#"
    },
    "credits": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "role"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "comment"
            }
          }
        ]
      }
    },
    "role": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "role_marker"
        },
        {
          "type": "SYMBOL",
          "name": "_role_name"
        },
        {
          "type": "STRING",
          "value": ": "
        },
        {
          "type": "SYMBOL",
          "name": "persons"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "_role_name": {
      "type": "PATTERN",
      "value": "[АБВГҐДЕЄЖЗИІЇЙКЛМНОПРСТУФХЦЧШЩЮЯ][абвгґдеєжзиіїйклмнопрстуфхцчшщьюя_\\-\\']*"
    },
    "role_marker": {
      "type": "STRING",
      "value": "#"
    },
    "persons": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "nickname"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ", "
              },
              {
                "type": "SYMBOL",
                "name": "nickname"
              }
            ]
          }
        }
      ]
    },
    "nickname": {
      "type": "PATTERN",
      "value": "[a-zA-ZабвгґдеєжзиіїйклмнопрстуфхцчшщьюяАБВГҐДЕЄЖЗИІЇЙКЛМНОПРСТУФХЦЧШЩЬЮЯ1-9_\\-\\']+"
    },
    "part": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "part_number"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "comment"
          }
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "page"
          }
        }
      ]
    },
    "part_number": {
      "type": "PATTERN",
      "value": "=== \\d+ ===\\n"
    },
    "page": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "page_number"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "sound"
              },
              {
                "type": "SYMBOL",
                "name": "separator"
              },
              {
                "type": "SYMBOL",
                "name": "text"
              }
            ]
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_empty"
          }
        }
      ]
    },
    "page_number": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "== \\d+ =="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "page_real_number"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "page_real_number": {
      "type": "PATTERN",
      "value": " \\(\\d+\\)"
    },
    "sound": {
      "type": "PATTERN",
      "value": ".*\\*\\d+\\n"
    },
    "separator": {
      "type": "PATTERN",
      "value": "---\\n"
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\n\\=].*\\n"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
